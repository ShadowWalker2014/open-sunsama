{
  "name": "@chronoflow/utils",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./date": {
      "import": "./dist/date.js",
      "types": "./dist/date.d.ts"
    },
    "./validation": {
      "import": "./dist/validation.js",
      "types": "./dist/validation.d.ts"
    },
    "./errors": {
      "import": "./dist/errors.js",
      "types": "./dist/errors.d.ts"
    },
    "./api-key": {
      "import": "./dist/api-key.js",
      "types": "./dist/api-key.d.ts"
    },
    "./constants": {
